programa {

real num,multiplicacao,divisao

  funcao inicio() {

    //entrada

    escreva("Digite um número: ")
    leia (num)

    //processamento

    multiplicacao=num*2
    divisao=num/3

   


    //saída

     escreva ("O dobro de ", num, " é ",multiplicacao, "\n")
     escreva ("A terça parte ",num, " é ",divisao)
  }
}
