programa {

  inteiro valor1,valor2,soma

  funcao inicio() {
    // entrada

    escreva("Digite um valor: ")
    leia (valor1)

    escreva ("Digite outro valor: ")
    leia (valor2)


    //processamento

    soma=valor1+valor2


    //saída


    escreva ("A soma entre ", valor1, " e " ,valor2, " é igual a ", soma)






  }
}
