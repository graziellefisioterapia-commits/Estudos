programa {

  real n1,n2,soma
  

  funcao inicio() {
    
    //entrada
    escreva("Digite o primeiro numero decimal (ex: 3.5): ")
    leia(n1)
    escreva("Digite o segundo numero decimal (ex:2.25): ")
    leia(n2)

    //processamento

    soma=n1+n2

    //saída

    escreva ("A soma de ",n1, " e ",n2, " é: ",soma)

    
  }
}
