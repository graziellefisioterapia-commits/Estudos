programa {

  caracter letra
  inteiro codigo

  funcao inicio() {

    //entrada

    escreva("Letra do setor(F, V, M): ")
    leia(letra)

    escreva("Código do funcionáriao: ")
    leia(codigo)

    //saída
    
    escreva("Letra do setor: ",letra,"\n")
    escreva("Codigo do funcionário: ",codigo,"\n")
    escreva("Funcionário do setor",letra,"código",codigo,".")


    
  }
}
